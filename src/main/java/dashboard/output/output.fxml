<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
            xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="dashboard.output.OutputPresenter">
    <children>
        <ScrollPane fx:id="scrollPane" layoutY="53.0" AnchorPane.topAnchor="58.0">
            <content>
                <AnchorPane>
                    <children>
                        <VBox fx:id="simplexesVBox"/>
                    </children>
                </AnchorPane>
            </content>
        </ScrollPane>
        <Button fx:id="start" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" text="Start">
            <font>
                <Font size="15.0"/>
            </font>
        </Button>
        <CheckBox fx:id="stepByStep" layoutX="76.0" layoutY="21.0" mnemonicParsing="false" selected="true"
                  text="Step by step">
            <font>
                <Font size="15.0"/>
            </font>
        </CheckBox>
        <Button fx:id="next" layoutX="197.0" layoutY="16.0" mnemonicParsing="false" text="next">
            <font>
                <Font size="15.0"/>
            </font>
        </Button>
        <Button fx:id="back" layoutX="255.0" layoutY="16.0" mnemonicParsing="false" onAction="#onClickBack" text="back">
            <font>
                <Font size="15.0"/>
            </font>
        </Button>
    </children>
</AnchorPane>
